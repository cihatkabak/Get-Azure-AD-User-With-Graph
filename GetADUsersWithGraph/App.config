<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--SharePoint-->
    <add key="TenantUrl" value="https://junointranet.sharepoint.com" />
    <!--SharePoint-->

    <!--Graph-->
    <add key="graph_client_id" value="write azure client-id" />
    <add key="graph_client_secret" value="write azure client-secret" />
    <!--Graph-->

    <!--Graph Requests Const-->
    <add key="grant_type" value="client_credentials" />
    <add key="resource" value="https://graph.microsoft.com" />
    <!--Graph Requests Const-->
    
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>